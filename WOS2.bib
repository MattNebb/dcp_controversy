@inproceedings{ WOS:000168167700004,
Author = {Cawley, RA},
Editor = {Bohlin, E and Brodin, K and Lundgren, A and Thorngren, B},
Title = {The impact of internet on communications regulatory models in Europe},
Booktitle = {CONVERGENCE IN COMMUNICATIONS AND BEYOND},
Year = {2000},
Pages = {47-62},
Note = {12th Biennial Conference of the
   International-Telecommunications-Society, STOCKHOLM, SWEDEN, JUN 20-24,
   1998},
Organization = {Int Telecommun Soc; Ctr Commun Res},
Abstract = {This paper examines some of the practical questions that the European
   policy and regulatory framework on telecommunications, digital
   television and broadcasting is facing as Internet develops. The paper
   begins by emphasising the underlying principles of the current policy
   framework. It examines general aspects of communications regulation and
   the main pillars of policy (telecommunications, broadcasting, internal
   market and competition policy) in a European context. It is then argued
   that Internet developments do not fundamentally change the basic aims of
   the regulatory framework, rather they tend to reveal where current
   practice is not adequately respecting basic principles. The main part of
   the paper examines a number of practical examples where regulatory
   uncertainty has arisen to illustrate the point. At the same time there
   have been calls for a more systematic treatment of policy in
   anticipation of convergence between telecommunications and broadcasting,
   driven to some extent by Internet developments. The final part of the
   paper briefly highlights some dangers of moving to an approach that
   attempts to be more systematic.},
