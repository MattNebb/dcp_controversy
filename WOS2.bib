@article{ WOS:000853177100001,
Author = {Lundqvist, Bjorn},
Title = {An access and transfer right to data-from a competition law
   perspective(dagger)},
Journal = {JOURNAL OF ANTITRUST ENFORCEMENT},
Abstract = {While business users face difficulties accessing and porting data on
   platforms, the Digital Markets Act has been hailed as the legislative
   tool enabling business users access and transfer the data they have
   generated on platforms controlled by gatekeepers. The tool provided by
   the Digital Markets Act is discussed in this article and the author
   argue that business users should have a more elaborated right to first
   access the data they produce on platforms and in ecosystems, and
   secondly transfer such data from platform to platform, cloud to cloud,
   thing to thing or in-house. A right to access and transfer data could
   have several benefits; it benefits dissemination of data, creativity and
   innovation in connected markets and it promotes competition between
   platforms, clouds and ecosystem providers. Creativity will be enhanced
   because necessary data-being the raw material for new innovations-will
   be more broadly disbursed. It will also benefit consumers having a
   disbursed and disseminated data commons for the development of ideas,
   innovations, and the exchange of knowledge.},
