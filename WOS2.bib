Title = {Review of monopoly models and implications for antitrust},
Booktitle = {8TH INTERNATIONAL CONFERENCE ON INFORMATION TECHNOLOGY AND QUANTITATIVE
   MANAGEMENT (ITQM 2020 \& 2021): DEVELOPING GLOBAL DIGITAL ECONOMY AFTER
   COVID-19},
