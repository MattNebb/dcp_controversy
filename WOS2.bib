@article{ WOS:000457135300013,
Author = {Binns, Reuben and Zhao, Jun and van Kleek, Max and Shadbolt, Nigel},
Title = {Measuring Third-party Tracker Power across Web and Mobile},
Journal = {ACM TRANSACTIONS ON INTERNET TECHNOLOGY},
Year = {2018},
Volume = {18},
Number = {4},
Month = {NOV},
Abstract = {Third-party networks collect vast amounts of data about users via
   websites and mobile applications. Consolidations among tracker companies
   can significantly increase their individual tracking capabilities,
   prompting scrutiny by competition regulators. Traditional measures of
   market share, based on revenue or sales, fail to represent the tracking
   capability of a tracker, especially if it spans both web and mobile.
   This article proposes a new approach to measure the concentration of
   tracking capability, based on the reach of a tracker on popular websites
   and apps. Our results reveal that tracker prominence and
   parent-subsidiary relationships have significant impact on accurately
   measuring concentration.},
