@article{ WOS:000848635000003,
Author = {Andriychuk, Oles},
Title = {Shifting the digital paradigm: Towards a sui generis competition policy},
Journal = {COMPUTER LAW \& SECURITY REVIEW},
Year = {2022},
Volume = {46},
Month = {SEP},
Abstract = {EU digital competition policy is in the process of a paradigmatic
   transformation. The main legal instrument, epitomising the rationale of
   the reform is the EU Digital Markets Act (DMA). This article
   conceptualises the broader theoretical foundations of EU digital
   competition policy and law, illustrating the developed hypotheses by
   examining the selected procedural novelties of the DMA. The main
   normative proposition, underpinning this article is that the emerging
   more proactive approach to digital markets reflects an objective
   socio-economic and political trend. This implies that the purified,
   inwards-oriented perception of competition policy will be gradually
   converting towards a more polycentric modality. An earlier version of
   this article has been awarded the prize by the Portuguese Competition
   Authority 2021 ``Competition Policy Award for academic work in
   Competition Policy{''}.(c) 2022 Oles Andriychuk. Published by Elsevier
   Ltd. All rights reserved.},
