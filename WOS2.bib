@article{ WOS:000745564300001,
Author = {Paelo, Anthea and Roberts, Simon},
Title = {Competition and Regulation of Mobile Money Platforms in Africa: A
   Comparative Analysis of Kenya and Uganda},
Journal = {REVIEW OF INDUSTRIAL ORGANIZATION},
Year = {2022},
Volume = {60},
Number = {3, SI},
Pages = {463-489},
Month = {MAY},
Abstract = {We analyze the competition issues in mobile money in Kenya-where
   Safaricom is the market leader-and in Uganda: where the market is
   duopolistic. We evaluate the multi-sided platform characteristics that
   have underpinned the rapid growth of the lead mobile money platforms and
   that also imply that the market leaders have substantial market power
   and can act as gatekeepers. The conduct of the lead firms is assessed
   along with competition measures and regulatory interventions made in
   each country. The analysis highlights the importance of-and challenges
