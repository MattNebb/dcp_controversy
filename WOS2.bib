@article{ WOS:000389698900001,
Author = {Edelman, Benjamin and Lai, Zhenyu},
Title = {Design of Search Engine Services: Channel Interdependence in Search
   Engine Results},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2016},
Volume = {53},
