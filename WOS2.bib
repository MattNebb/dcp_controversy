@article{ WOS:000545477800006,
Author = {Bougette, Patrice and Budzinski, Oliver and Marty, Frederic},
Title = {Exploitative Abuse and Abuse of Economic Dependence: What Can We Learn
   From an Industrial Organization Approach?},
Journal = {REVUE D ECONOMIE POLITIQUE},
Year = {2019},
Volume = {129},
