@article{ WOS:000658782800006,
Author = {Aron, Shaurya},
Title = {Innovation: An Antitrust Trojan Horse?},
Journal = {WORLD COMPETITION},
Year = {2021},
Volume = {44},
Number = {2},
Pages = {227-244},
Month = {JUN},
Abstract = {Innovation is regarded as the spearhead of consumer benefit in a
   competitive market. However, a strategy of predatory innovation presents
   the innovator with a shower of opportunities to close down the market
   and drive out competitors, thereby directly affecting consumer
   sovereignty. An inquiry into the relationship between innovation and
   market structures is not only important to assess the impact of such
   forms of the market on innovative activities, but also for a
   well-founded and evidenced framing of principles of antitrust laws in
   respect of Intellectual Property Rights (`IPRs'). Therefore, this
   article makes a case for the legal recognition, across jurisdictions
   including India, of predatory innovation in the context of high-tech
   markets to uphold the objectives of the antitrust regime, i.e., consumer
   ivelfare. The article traces the progressive steps taken by developed
   jurisdiction in this context and highlights how a similar approach could
   be adopted by India, fitting its current framework.
   This article, firstly, discusses the impact the market structure has on
   the motives and incentives for a manufacturer to innovate; secondly,
   brings forward the anti-competitive nature of predatory innovation in
   high tech markets; thirdly, advocates for the need for legal recognition
   of the conduct of Predatory Innovation while noting the
   counterarguments; lastly, concludes with identifying the effects of
   recognizing Predatory Innovation as anti-competitive conduct.},
Publisher = {KLUWER LAW INT},
Address = {ZUIDPOOLSINGEL 2, PO BOX 316, 2400 AH ALPHEN AAN DEN RIJN, NETHERLANDS},
Type = {Article},
Language = {English},
Affiliation = {Aron, S (Corresponding Author), Rajiv Gandhi Natl Univ Law, Patiala, Punjab, India.
   Aron, Shaurya, Rajiv Gandhi Natl Univ Law, Patiala, Punjab, India.},
ISSN = {1011-4548},
EISSN = {1875-8436},
Keywords = {Innovation; Predatory; Market Structures; Monopoly; Intellectual
   Property Rights; India; High-tech Markets; Interoperability; Platforms;
