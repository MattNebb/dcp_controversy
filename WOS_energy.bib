@article{ WOS:000337008100003,
Author = {Meier, Jens and Lachmann, Maik and Hoffjan, Andreas and Loeper, Nadja},
Title = {Valuation of energy networks in the context of concession agreement
   acquisitions - Status quo and perspectives for a reorientation},
Journal = {BETRIEBSWIRTSCHAFTLICHE FORSCHUNG UND PRAXIS},
Year = {2014},
Volume = {66},
Number = {3},
Pages = {283-298},
Month = {MAY-JUN},
Abstract = {As part of the 4th antitrust novella, the German legislator regulated
   the maximum duration of concession agreements for companies operating
   electricity and gas networks. As a consequence, a large portion of the
   20,000 existing, concession agreements are currently about to expire.
   Local municipalities are expected to decide whether or not to call for
   offers from operating companies to acquire concessions for existing
   distribution networks. A central issue in this case is the determination
   of a ``fair{''} purchase price for the distribution network. This paper
   examines the impact of different legal form interpretations on the
   valuation methods used for energy networks. In this context, we analyse
   under which circumstances the valuation concept of the objectified
   capitalized earnings value may be substituted by more adequate valuation
   methods. In particular, a modification of the valuation perspective and
   the consideration of acquirer-specific synergies are discussed.},
