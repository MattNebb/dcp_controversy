@article{ WOS:000244848800040,
Author = {Pittman, Russell},
Title = {Restructuring the Russian electricity sector: Re-creating California?},
Journal = {ENERGY POLICY},
Year = {2007},
Volume = {35},
Number = {3},
Pages = {1872-1883},
Month = {MAR},
Abstract = {The Russian Federation has begun restructuring its electricity sector,
   following the standard restructuring model of complete vertical
   separation of generation from transmission, with the aim of creating
   competition in regional generation markets. This paper examines the
   structure of the six principal regional generation markets that are in
   their early stages of development and argues that they are likely to be
   characterized by high levels of market power on the part of individual
   privatized generation companies, especially during the peak winter
   demand season. These levels-considerably higher than those that caused
   competitive problems in California-seem to create a serious risk of
   price spikes in deregulated wholesale electricity markets, and thus of
   significant price increases to consumers of electricity. Published by
   Elsevier Ltd.},
Publisher = {ELSEVIER SCI LTD},
