@article{ WOS:000253725700008,
Author = {Urzua, Carlos M.},
Title = {Consensus and dissension among Mexican economists},
Journal = {CEPAL REVIEW},
Year = {2007},
Number = {91},
Pages = {155-167},
Month = {APR},
Abstract = {This article identifies areas of agreement and disagreement among
   Mexican economists in relation to key issues of economics, on the basis
   of a survey of 360 members of the profession. The propositions eliciting
   the greatest consensus included: equity in income distribution, vigorous
   use of antitrust and consumer protection laws and the use of
   countercyclical fiscal policies. The most contentious issues were:
   private investment in the energy sector, the determinants of inflation,
   the minimum wage as a cause of unemployment and the objectives to be
   pursued by an independent central bank.},
