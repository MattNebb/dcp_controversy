@article{ WOS:000328097900004,
Author = {Davies, Stephen and Price, Catherine Waddams and Wilson, Chris M.},
Title = {Nonlinear Pricing and Tariff Differentiation: Evidence from the British
   Electricity Market},
Journal = {ENERGY JOURNAL},
Year = {2013},
Volume = {35},
Number = {1},
Pages = {57-77},
Abstract = {Liberalisation of the British household electricity market, in which
   previously monopolised regional markets were exposed to large-scale
   entry, is used as a natural experiment on oligopolistic nonlinear
   pricing. Each oligopolist offered a single two-part electricity tariff,
   but inconsistent with current theory, the two-part tariffs were
   heterogeneous in ways that cannot be attributed to explanations such as
   asymmetric costs or variations in brand loyalty. Instead, the evidence
   suggests that firms deliberately differentiated their tariff structures,
   resulting in market segmentation according to consumers' usage.},
