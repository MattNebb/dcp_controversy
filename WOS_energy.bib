@article{ WOS:000220762500004,
Author = {Chapman, D and Vossler, CA and Mount, TD and Barboni, V and Thomas, RJ
   and Zimmerman, RD},
Title = {Market efficiency, competition, and communication in electric power
   markets: experimental results},
Journal = {ECOLOGICAL ECONOMICS},
Year = {2004},
Volume = {48},
Number = {3},
Pages = {317-327},
Month = {MAR 31},
Abstract = {Economic theory gives no clear indication of the minimum number of
   producers necessary for a market to define competitive price-quantity
   equilibria, which approximate price equal to marginal cost. Previous
   work and Federal Energy Regulatory Commission (FERC) guidelines
   generally suggest that 6-10 suppliers may be workably competitive. Our
   experiments with PowerWeb suggest that a higher number of suppliers may
   be necessary to approximate competitive market solutions, this in the
   absence of any communication among producers. As communications rules
   are altered to parallel differing types of antitrust enforcement, market
   results with 24 participants approach pure monopoly values. (C) 2004
   Elsevier B.V. All rights reserved.},
