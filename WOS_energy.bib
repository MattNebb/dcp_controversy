@article{ WOS:000445328700011,
Author = {Reichelstein, Stefan and Sahoo, Anshuman},
Title = {Relating Product Prices to Long-Run Marginal Cost: Evidence from Solar
   Photovoltaic Modules},
Journal = {CONTEMPORARY ACCOUNTING RESEARCH},
Year = {2018},
Volume = {35},
Number = {3},
Pages = {1464-1498},
Month = {FAL},
Abstract = {A basic tenet of microeconomics is that for a competitive industry in
   equilibrium the market price of a product will be equal to its marginal
   cost. This paper develops a model framework and a corresponding
   empirical inference procedure for estimating long-run marginal cost in
   industries where production costs decline over time. In the context of
   the solar photovoltaic (PV) module industry, we rely primarily on
   firm-level financial accounting data to estimate the long-run marginal
   cost of PV modules for the years 2008-2013. During those years, the
   industry experienced both unprecedented price declines and significant
   expansions of manufacturing capacity. We compare the trajectory of
   average sales prices with the estimated long-run marginal costs in order
   to quantify the extent to which actual price declines were attributable
   to reductions in production costs. The trajectory of estimated product
   costs is then extrapolated to forecast an equilibrium trend line for
   future PV module prices.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Article},
Language = {English},
Affiliation = {Reichelstein, S (Corresponding Author), Stanford Univ, Stanford, CA 94305 USA.
   Reichelstein, S (Corresponding Author), Univ Mannheim, Mannheim, Germany.
   Reichelstein, Stefan; Sahoo, Anshuman, Stanford Univ, Stanford, CA 94305 USA.
   Reichelstein, Stefan, Univ Mannheim, Mannheim, Germany.},
DOI = {10.1111/1911-3846.12319},
ISSN = {0823-9150},
EISSN = {1911-3846},
Keywords = {Long-run marginal cost; Cost estimation; Learning-by-doing; Price
   dynamics},
Keywords-Plus = {SILICON PHOTOVOLTAICS; LEARNING-CURVE; DECISION RULES; INVESTMENT;
   DYNAMICS; COMPETITION; INDUSTRY},
Research-Areas = {Business \& Economics},
Web-of-Science-Categories  = {Business, Finance},
Author-Email = {reichelstein@stanford.edu},
Funding-Acknowledgement = {Chartered Professional Accountants of Canada; Steyer-Taylor Center for
   Energy Policy and Finance; U.S. Department of Energy (DOE)},
Funding-Text = {Accepted by Shane Dikolli. An earlier version of this paper was
   presented at the 2016 Contemporary Accounting Research Conference,
   supported by the Chartered Professional Accountants of Canada. For
   helpful comments, we thank seminar participants at the following
   universities: Stanford, New York University, Southern Methodist, Vienna,
