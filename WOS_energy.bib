@inproceedings{ WOS:000426716800078,
Author = {Yang, Yujing and Chen, Miaomiao},
Editor = {Fang, Z},
Title = {Toward A Sustainable Clean Energy Future: Lesson from Chinese
   Intellectual Property Litigations},
Booktitle = {PROCEEDINGS OF THE 2017 INTERNATIONAL CONFERENCE ON ECONOMICS AND
   MANAGEMENT, EDUCATION, HUMANITIES AND SOCIAL SCIENCES (EMEHSS 2017)},
