@article{ WOS:000577289900001,
Author = {Wang, Yuting},
Title = {The anti-monopoly regulation of reverse payment patent settlement
   agreements in the pharmaceutical industry in China{*}},
Journal = {ASIA PACIFIC LAW REVIEW},
