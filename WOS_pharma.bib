@article{ WOS:000592383700002,
Author = {Carrier, Michael A. and Lemley, Mark A. and Miller, Shawn},
Title = {Playing Both Sides? Branded Sales, Generic Drugs, and Antitrust Policy},
Journal = {HASTINGS LAW JOURNAL},
