@article{ WOS:000414166000001,
Author = {Gallini, Nancy},
Title = {Do patents work? Thickets, trolls and antibiotic resistance},
Journal = {CANADIAN JOURNAL OF ECONOMICS-REVUE CANADIENNE D ECONOMIQUE},
Year = {2017},
Volume = {50},
