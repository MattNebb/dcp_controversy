@article{ WOS:000736048800003,
Author = {Katz, Michael L.},
Title = {Big Tech mergers: Innovation, competition for the market, and the
   acquisition of emerging competitors},
Journal = {INFORMATION ECONOMICS AND POLICY},
Year = {2021},
Volume = {54},
Month = {MAR},
Abstract = {There is broad concern that merger policy toward Big Tech has been too
   lenient. Big Tech typically operates in markets characterized by
   innovation-driven ``competition for the market.{''} I show that this
   fact provides a rationale for heightened scrutiny of incumbents'
   acquisitions of emerging or potential competitors. I also address the
   widespread argument that permissive merger policy promotes innovative
   entry by facilitating entry for buyout. I show that permissive merger
   policy can also discourage entrant innovation. One way is by diminishing
   entrants' incentives to invest in marginal product improvements when
   such improvements reduce the gains from merger. A second way is by
   facilitating incumbency for buyout, under which an incumbent makes
   investments in order to extract rents from an entrant through merger.
   (c) 2020 Published by Elsevier B.V.},
Publisher = {ELSEVIER},
