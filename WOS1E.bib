@article{ WOS:000614261500002,
Author = {Filkin, M. E.},
Title = {Diagnostic system for cooperative pricing detection in retail markets},
Journal = {EKONOMIKA I MATEMATICESKIE METODY-ECONOMICS AND MATHEMATICAL METHODS},
