@article{ WOS:000180594800003,
Author = {Button, K},
Title = {Does the theory of the `core' explain why airlines fail to cover their
   long-run costs of capital?},
Journal = {JOURNAL OF AIR TRANSPORT MANAGEMENT},
Year = {2003},
Volume = {9},
