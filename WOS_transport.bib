@article{ WOS:000303050700010,
Author = {Hamoudi, Hamid and Risueno, Marta},
Title = {THE EFFECTS OF ZONING IN SPATIAL COMPETITION},
Journal = {JOURNAL OF REGIONAL SCIENCE},
Year = {2012},
Volume = {52},
Number = {2},
Pages = {361-374},
Month = {MAY},
Abstract = {This paper considers a location model to illustrate the effect of zoning
   on competition. A planner is in charge of designing a city in a circular
   space where firms and consumers are located on different sides. With
   this type of market configuration, equilibrium in location under concave
   transport costs is proved. Then, a welfare function with different
   weights attached to consumer and firm surpluses is introduced to
   highlight zoning regulation as an influential competition policy tool.
   Depending on the regulator's political profiles and the demand, it is
   shown that zoning can lead to strong, weak, or moderate competition.},
