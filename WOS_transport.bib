@inproceedings{ WOS:000258321200134,
Author = {Spada, Mariagrazia},
Book-Group-Author = {IEEE},
Title = {GNSS optimizing intercontinental liberalization of air transport},
Booktitle = {2008 IEEE AEROSPACE CONFERENCE, VOLS 1-9},
